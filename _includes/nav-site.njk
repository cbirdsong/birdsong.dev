<nav class="nav nav-primary" id="main-menu" aria-label="Main Menu">
  {% set navPages = collections.all | eleventyNavigation %}
  <ul>
    {%- for item in navPages %}
      <li {% if item.url == page.url %} class="active" {% endif %}>
        <a href="{{ item.url | url }}" {% if item.url == page.url %} class="active" {% endif %}>{{ item.title }}</a>
      </li>
    {%- endfor %}
  </ul>
</nav>